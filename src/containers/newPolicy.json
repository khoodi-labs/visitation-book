{
	"Version": "2012-10-17",
	"Id": "Policy1585766847625",
	"Statement": [
        {
			"Sid": "Stmt1585766846125",
			"Effect": "Deny",
			"Principal": "*",
			"Action": "s3:*",
			"Resource": "arn:aws:s3:::newvision-media/*",
            "Condition": {
                "StringEquals": {
                  "s3:ExistingObjectTag/extension": ".exe"
                }
              }
		},
		{
			"Sid": "Stmt1585766846125",
			"Effect": "Allow",
			"Principal": "*",
			"Action": "s3:*",
			"Resource": "arn:aws:s3:::newvision-media/*"
		}
	]
}